	PROCEDURE synctree
		* Sync tree to current SetID
		FOR EACH oNode IN THIS.oTree.Nodes
			IF SUBSTR(oNode.Key, 3, 10) == THIS.cCurrentSetID
				oNode.Selected = .T.
				oNode.EnsureVisible()
				EXIT
			ENDIF
		ENDFOR
		
		
	ENDPROC

